package arraysrecta;
import java.util.*;
class Gruhaa
{
	public static void Solution(String sa,int k)
	{
		int i,j;
		int n=sa.length();
		int c=n-k;
		String st[]=new String[c];
		for(i=1,j=i+k;i<=n-k;i++)
		{
			String st[i]=sa.substring(i,j);
		}
		
		String saee[]=new String[c];
		for(i=1;i<=c;i++)
		{
			saee[i]=st[i];
			
		}
		Arrays.sort(saee);
		System.out.println(saee[1]);
		System.out.println(saee[c]);
		
	}
	
}

public class Hacker {

	public static void main(String[] args) {
		Scanner s=new Scanner(System.in);
		String sa=s.nextLine();
		int k=s.nextInt();

	}

}
